{
  "name": "lk-feed",
  "displayName": "Lk feed",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build && (cd ./build/chrome-mv3-prod && zip -r ../../chrome-mv3-prod.zip .)",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.12.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.7.7",
    "plasmo": "0.89.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "scripting",
      "contextMenus",
      "storage",
      "unlimitedStorage"
    ],
    "host_permissions": [
      "https://www.linkedin.com/*",
      "https://stopwordapi.com/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
  }
}
